{
    "name"          : "Default",
    "displayName"   : "Admin Application View (Default)",
    "description"   : "Default Admin View",
    "version"       : "0.0.1",
    "repo"          : "https://github.com/h-w-f/view-admin-default",
    "dependencies"  : {
        "core"          : {
            "name"      : "Core",
            "version"   : "0.0.1",
            "repo"      : "https://github.com/h-w-f/core"
        },
        "application"  : {
            "name"      : "Admin",
            "version"   : "0.0.1",
            "repo"      : "https://github.com/h-w-f/application-admin"
        },
        "components"    : [],
        "packages"      : [],
        "middlewares"   : [],
        "views"         : []
    },
    "settings"      : {
        "cache": false,
        "layout": "/layout/view.html",
        "head": {
            "title": "Admin - ",
            "meta": {
                "viewport": "width=device-width, initial-scale=1, shrink-to-fit=no",
                "description": "Admin Views",
                "author": "Guru",
                "keywords": "HTML, CSS, JavaScript"
            },
            "link": {
                "href": [
                    "Admin/Default/css/bootstrap.min.css",
                    "Admin/Default/css/font-awesome.min.css",
                    "Admin/Default/css/PNotify.min.css",
                    "Admin/Default/css/icheck.css",
                    "Admin/Default/css/core.css"
                ]
            },
            "script": {
                "src": [
                    "Admin/Default/js/jquery-3.5.1.min.js",
                    "Admin/Default/js/bootstrap.min.js"
                ]
            }
        },
        "body": {
            "params": "id=\"body\" class=\"body\"",
            "jsscript": ""
        },
        "footer": {
            "params": "class=\"footer\"",
            "script": {
                "src": [
                    "Admin/Default/js/PNotify.min.js",
                    "Admin/Default/js/sortable.min.js",
                    "Admin/Default/js/swal2.min.js"
                ]
            },
            "jsscript": "PNotify.defaultModules.set(PNotifyBootstrap4, {});PNotify.defaultModules.set(PNotifyFontAwesome5, {});"
        }
    }
}