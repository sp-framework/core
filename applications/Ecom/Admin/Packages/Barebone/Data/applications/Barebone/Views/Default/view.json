{
	"name"          : "Default",
	"displayName"   : "Barebone Application View (Default)",
	"description"   : "Default Barebone View",
	"version"       : "0.0.1",
	"repo"          : "https://.../view-admin-default",
	"dependencies"  : {
		"core"          : {
			"name"      : "Core",
			"version"   : "0.0.1",
			"repo"      : "https://.../core"
		},
		"application"  	: {
			"name"      : "Barebone",
			"version"   : "0.0.1",
			"repo"      : "https://.../application-admin"
		},
		"components"    : [],
		"packages"      : [],
		"middlewares"   : [],
		"views"         : []
	},
	"settings"      : {
		"cache"		: false,
		"layout"	: "default",
		"head": {
			"title"		: "Barebone",
			"meta"		: {
				"charset"		: "UTF-8",
				"viewport"		: "width=device-width, initial-scale=1, shrink-to-fit=no",
				"description"	: "Barebone Views",
				"author"		: "Guru",
				"keywords"		: "HTML, CSS, JavaScript"
			},
			"link"		: {
				"href"	: [
					"Barebone/Default/css/bootstrap.min.css",
					"Barebone/Default/css/font-awesome.min.css",
					"Barebone/Default/css/PNotify.min.css",
					"Barebone/Default/css/icheck.css",
					"Barebone/Default/css/core.css"
				]
			},
			"script"	: {
				"src"	: [
					"Barebone/Default/js/jquery-3.5.1.min.js",
					"Barebone/Default/js/bootstrap.min.js"
				]
			}
		},
		"body": {
			"params"	: "id=\"body\" class=\"body\"",
			"jsscript"	: ""
		},
		"footer": {
			"params"	: "class=\"footer\"",
			"script"	: {
				"src"	: [
					"Barebone/Default/js/PNotify.min.js",
					"Barebone/Default/js/sortable.min.js",
					"Barebone/Default/js/swal2.min.js"
				]
			},
			"jsscript"	: "PNotify.defaultModules.set(PNotifyBootstrap4, {});PNotify.defaultModules.set(PNotifyFontAwesome5, {});"
		}
	}
}